{
  "name": "@orval/query",
  "version": "6.20.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "tsup": {
    "entry": ["./src/index.ts"],
    "sourcemap": true,
    "shims": true,
    "clean": true,
    "dts": true
  },
  "scripts": {
    "build": "tsup --format esm,cjs",
    "dev": "tsup --format esm,cjs --watch src",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "@orval/core": "6.20.0",
    "lodash.omitby": "^4.6.0"
  },
  "devDependencies": {
    "@types/lodash.omitby": "^4.6.7"
  }
}
